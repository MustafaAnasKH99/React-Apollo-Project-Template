{
  "name": "react-apollo-project-template",
  "version": "1.0.0",
  "description": "Template for any React and Apollo Project `git clone`, `npm i`, and `npm start`",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "cd server && npm start",
    "client": "cd client && npm start",
    "start": "concurrently --kill-others-on-fail --names \"server,client\" \"npm run server\" \"npm run client\"",
    "postinstall": "cd client && npm install && cd .. && cd server && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MustafaAnasKH99/React-Apollo-Project-Template.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MustafaAnasKH99/React-Apollo-Project-Template/issues"
  },
  "homepage": "https://github.com/MustafaAnasKH99/React-Apollo-Project-Template#readme",
  "dependencies": {
    "concurrently": "^4.1.2"
  }
}
